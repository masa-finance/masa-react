  "license": "MIT",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "unpkg": "./dist/browser/masa-react.cjs2.min.js",
  "module": "./dist/browser/masa-react.amd.min.js",
  "files": [
    "dist"
