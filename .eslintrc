{
  "root": true,
  "extends": [
    "masa",
    "plugin:@tanstack/eslint-plugin-query/recommended"
  ],
  "plugins": [
    "@tanstack/query"
  ],
  "rules": {
    "no-console": "off"
  },
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
      "parser": "@typescript-eslint/parser"
    },
    {
      "files": [
        "**/*.stories.*"
      ],
      "rules": {
        "import/no-anonymous-default-export": "off"
      }
    }
  ]
}
